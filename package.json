{
  "name": "shannic",
  "description": "The music app you deserve",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "bun x cypress run",
    "test:unit": "bun test",
    "lint": "bun x @biomejs/biome check --write ./src",
    "assets:generate": "bun x capacitor-assets generate",
    "android:debug": "bun run build && bun x cap copy android && bun x cap run android",
    "android:copy": "bun run build && bun x cap copy android"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.2",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capgo/inappbrowser": "^8.1.10",
    "@ionic/vue": "^8.7.17",
    "@ionic/vue-router": "^8.7.17",
    "ionicons": "^8.0.13",
    "node-vibrant": "^4.0.4",
    "pinia": "^3.0.4",
    "rxdb": "^16.21.1",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^8.0.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^15.9.0",
    "jsdom": "^27.4.0",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
